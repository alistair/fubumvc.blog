<div class="managearticlefilters">
  <div>
    <select class="filter" >
      ${this.Option(x => x.ShowDraft, "All", null)}
      ${this.Option(x => x.ShowDraft, "Draft", "True")}
      ${this.Option(x => x.ShowDraft, "Published", "False")}
    </select>
    <select>
      <option>Bulk Actions</option>
      <option>Delete</option>
      <option>Publish</option>
    </select>
  </div>

  <paging totalPages="Model.TotalPages"/>
</div>